  object sdsClientes: TFDQuery
    Connection = SQLConexao
    SQL.Strings = (
      'select c.*,'
      'case when c.cliente_tipo = '#39'F'#39' then c.cliente_cpf'
      'else c.cliente_cnpj end "Tipo de Cliente"'
      'from clientes c'
      'order by c.cliente_id asc')
    Left = 38
    Top = 82
